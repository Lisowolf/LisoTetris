@using Engine;

@switch (State)
{
    case Pixel.Empty:
        <div class="pixel" />
        break;
    case Pixel.Filled:
        <div class="pixel pixel-filled" />
        break;
    case Pixel.CurrentBlock:
        <div class="pixel pixel-current-block" />
        break;
}

@code {
    [Parameter]
    public Pixel State { get; set; }
}